    </group>

    <group if = "$(eval launch_nuc == 'sony_cam3')">
        <include file="$(find sony_cam)/launch/viewcam.launch">
            <arg name="camera_name" value="$(arg launch_nuc)" />
        </include>
    </group>

        <group if = "$(eval launch_nuc == 'sony_cam4')">
        <include file="$(find sony_cam)/launch/viewcam.launch">
            <arg name="camera_name" value="$(arg launch_nuc)" />
        </include>
    </group>

        <group if = "$(eval launch_nuc == 'sony_cam5')">
        <include file="$(find sony_cam)/launch/viewcam.launch">
            <arg name="camera_name" value="$(arg launch_nuc)" />
        </include>
    </group>
</launch>

